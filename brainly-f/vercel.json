{
  "version": 2,
  "builds": [
    {
      "src": "brainly-f/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "cd brainly-f && npm install && npm run build"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/brainly-f/dist/index.html" }
  ]
}